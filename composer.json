{
	"name": "showpro/silverstripe-seo-images",
	"description": "Automaticly optimise and generates WebP images of all uploaded & resampled images in SilverStripe",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/mi32dogs/silverstripe-seo-images",
	"keywords": ["silverstripe", "upload", "image", "compress", "optimisation", "webp"],
	"license": "MIT",
	"authors": [
		{
			"name": "Rob van Tuin"
		}
	],
	"support": {
		"issues": "https://github.com/mi32dogs/silverstripe-seo-images/issues"
	},
    "require": {
        "silverstripe/framework": "^4.2",
        "silverstripe/assets": "*",
        "spatie/image-optimizer": "^1.2",
        "rosell-dk/webp-convert": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "showpro\\SEOImages\\": "src/"
        }
    }
}
